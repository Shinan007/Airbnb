<%layout('layouts/boilerplate.ejs')-%>

    <body>
        <%-include('../includes/navbar.ejs')%>
            <div class="wrapper">
                <%for(obj of Listings){%>
                    <a href="/listings/<%=obj.id%>/show" class="listing-links">
                        <div class="card1">
                            <img src="<%=obj.image%>">
                            <div class="card1-body">
                                <h5 class="card1-text">
                                    <%=obj.title%>
                                </h5>
                                <p>
                                    &#8377 <%=obj.price%>/night
                                </p>
                            </div>
                        </div>
                    </a>
                    <%}%>
            </div>
            <%-include('../includes/footer.ejs')%>
    </body>